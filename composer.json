{
    "name": "nayzo/json-schema-validator-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle used to handle Json Schema validation for PSR7 & PSR17",
    "keywords": ["openapi", "http", "validation", "bundle", "json", "schema"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ala Eddine Khefifi",
            "email": "alakfpro@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nayzo\\JsonSchemaValidatorBundle\\": "src"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "ext-mbstring": "*",
        "league/openapi-psr7-validator": "^0.22",
        "nyholm/psr7": "^1.0",
        "symfony/event-dispatcher": "^5.4 || ^6.0 || ^7.0 || ^8.0",
        "symfony/framework-bundle": "^5.4 || ^6.0 || ^7.0 || ^8.0",
        "symfony/psr-http-message-bridge": "^2.0 || ^7.0 || ^8.0",
        "symfony/string": "^5.4 || ^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.60",
        "dg/bypass-finals": "^1.6",
        "phpunit/phpunit": "^9.0 || ^10.0 || ^11.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Nayzo\\JsonSchemaValidatorBundle\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
